<?xml version='1.0' encoding='utf-8'?>
<widget android-packageName="com.ACS" id="co.qamc.qamc" version="1.0.2" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://qamc.co/ns/1.0">
    <name>ACS2.0046</name>
    <description>
        QPack test
    </description>
    <author email="colin@qamc.co" href="http://qamc.co">
        Advanced Catering Solutions
    </author>
    <content src="index.html" />
    <preference name="orientation" value="portrait" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-kiosk" spec="https://github.com/hkalina/cordova-plugin-kiosk.git" />
    <plugin name="cordovarduino" spec="https://github.com/xseignard/cordovarduino" />
    <engine name="android" spec="^6.2.0" />
</widget>
